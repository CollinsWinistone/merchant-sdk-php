{
    "name": "collinswinistone/merchant-sdk-php",
    "description": "PayPal Merchant SDK for PHP (Forked Version)",
    "keywords": ["paypal", "php", "sdk"],
    "homepage": "https://github.com/CollinsWinistone/merchant-sdk-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Collins Winistone",
            "homepage": "https://github.com/CollinsWinistone"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "collinswinistone/sdk-core-php": "dev-main"
    },
    "autoload": {
        "psr-0": {
            "PayPal\\Service": "lib/",
            "PayPal\\CoreComponentTypes": "lib/",
            "PayPal\\EBLBaseComponents": "lib/",
            "PayPal\\EnhancedDataTypes": "lib/",
            "PayPal\\PayPalAPI": "lib/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/CollinsWinistone/merchant-sdk-php"
        }
    ]
}
